<Window x:Class="WUDownloaderGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WUDownloaderGUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="5*"/>
            <RowDefinition Height="95*"/>
        </Grid.RowDefinitions>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Button x:Name="Import" Content="Import" VerticalContentAlignment="Center" Click="Import_Click" Height="30"/>
            <Button x:Name="Export" Content="Export" VerticalContentAlignment="Center" Click="Export_Click" Height="30" Grid.Column="1"/>
        </Grid>
        <DataGrid x:Name="grid"
                  Grid.Row="1"/>
    </Grid>
</Window>
